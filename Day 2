
a = float(input("Enter the value of variable a: "))
b = float(input("Enter the value of variable b: "))
floor_division_result = a // b
power_result = a ** b
equal_check_result = (a == b)

# Print the results
print("1. Floor Division:", floor_division_result)
print("2. a power b:", power_result)
print("3. Are the variables equal?", equal_check_result)
